<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Thickness x:Key="ButtonBorderThemeThickness">2</Thickness>
    <SolidColorBrush x:Key="ButtonBackgroundThemeBrush" Color="#33000000" />
    <SolidColorBrush x:Key="ButtonBorderThemeBrush" Color="Transparent" />
    <SolidColorBrush x:Key="ButtonDisabledBackgroundThemeBrush" Color="#33000000" />
    <SolidColorBrush x:Key="ButtonDisabledBorderThemeBrush" Color="Transparent" />
    <SolidColorBrush x:Key="ButtonDisabledForegroundThemeBrush" Color="#66000000" />
    <SolidColorBrush x:Key="ButtonForegroundThemeBrush" Color="#FF000000" />
    <SolidColorBrush x:Key="ButtonPointerOverBackgroundThemeBrush" Color="#33000000" />
    <SolidColorBrush x:Key="ButtonPointerOverBorderThemeBrush" Color="#66000000" />
    <SolidColorBrush x:Key="ButtonPointerOverForegroundThemeBrush" Color="#FF000000" />
    <SolidColorBrush x:Key="ButtonPressedBackgroundThemeBrush" Color="#66000000" />
    <SolidColorBrush x:Key="ButtonPressedForegroundThemeBrush" Color="#FF000000" />
    <SolidColorBrush x:Key="ButtonPressedBorderThemeBrush" Color="Transparent" />
</ResourceDictionary>